import java.util.*;
class Sum_of_Digit_of_Number 
{
    public static void main(String[]args)
    {
        System.out.print(" Enter a Number : ");
        Scanner sc= new Scanner(System.in);
       int a =sc.nextInt();
        int sum =0;
        while(a!=0){
            sum+= a%10;
            a=a/10;
        }
        System.out.println(" The sum of digit of the number : "+ sum );
    }
}
    
    
